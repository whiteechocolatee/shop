include headerModule 

include navigationHeaderModule

div(class='wrapper')
    h1(class='big-logo absolution') D R E S S
    div(class='container-lg main-content')
        h1(class='category-title')= category[0]['category']
        div(class='category-gallery')
            div(class='filters-side')
                nav(class='filtration-nav')
                    ul
                        li(class='filtration-li selected' data-filter='all') Всё
                        each value in types 
                            li(class='filtration-li' data-filter= value['data_type'])= value['type']
            div(class='cards-container')
                each val,index in goods[0]
                    include cardsModule
            div(class='pagination')
                each val,index in goods[1]
                    include paginate
    script(src='/js/productNavigation.js') 
include footerModule