include header 

include navigationHeader

div(class='wrap')
    div(class='container')
        div(class='col col-lg-12')
            each valGoods,index in category ? category : ['There is no value']
                h2(class='category-name')
                    a( class='category-link' href='cat?id='+valGoods['id'])= valGoods['category']
                div(class="cards-container")
                    each val,index in goods
                        if valGoods['id'] == val['category']
                                include cards

    include navigationPanel
include footer 